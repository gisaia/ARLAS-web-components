{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "arlas-web-components-demo": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              "src/styles.css",
              "node_modules/mapbox-gl/dist/mapbox-gl.css",
              "node_modules/@gisaia-team/mapbox-gl-draw/dist/mapbox-gl-draw.css",
              "node_modules/iv-viewer/dist/iv-viewer.css"
            ],
            "scripts": [

            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "arlas-web-components:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "arlas-web-components:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "arlas-web-components:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [

            ],
            "styles": [
              "src/styles.css",
              "node_modules/mapbox-gl/dist/mapbox-gl.css",
              "node_modules/@gisaia-team/mapbox-gl-draw/dist/mapbox-gl-draw.css",
              "node_modules/iv-viewer/dist/iv-viewer.css"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": ["src/components/**/*.schema.json"]
          }
        }
      }
    },
    "arlas-web-components-demo-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "arlas-web-components:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "arlas-web-components": {
      "projectType": "library",
      "root": "projects/arlas-components",
      "sourceRoot": "projects/arlas-components/src",
      "prefix": "arlas",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/arlas-components/tsconfig.lib.json",
            "project": "projects/arlas-components/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/arlas-components/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/arlas-components/src/test.ts",
            "tsConfig": "projects/arlas-components/tsconfig.spec.json",
            "karmaConfig": "projects/arlas-components/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/arlas-components/tsconfig.lib.json",
              "projects/arlas-components/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "projects/arlas-components/src/lib/components/**/*.schema.json"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "arlas-web-components-demo",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "arlas",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "arlas"
    }
  }
}
